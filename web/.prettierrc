{
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 4,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js"],
            "options": {
                "tabWidth": 4
            }
        },
        {
            "files": ["*.json"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
